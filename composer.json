{
  "name": "webclient/ext-log",
  "type": "library",
  "description": "Logging extension for PSR-18 HTTP client",
  "keywords": ["http", "client", "http-client", "http-client-extension", "logging"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Dudarev",
      "email": "ivan@ddrv.ru",
      "homepage": "https://ddrv.ru"
    }
  ],
  "require": {
    "php": ">=7.0",
    "psr/http-client": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.7",
    "phpunit/phpunit": ">=6.5",
    "squizlabs/php_codesniffer": "^3.5",
    "webclient/fake-http-client": "^1.0"
  },
  "provide": {
    "psr/http-client-implementation": "1.0"
  },
  "suggest": {
    "psr/http-client-implementation": "Choice your favorite psr-18 implementation",
    "psr/log-implementation": "Choice your favorite psr-3 implementation"
  },
  "autoload": {
    "psr-4": {
      "Webclient\\Extension\\Log\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stuff\\Webclient\\Extension\\Log\\": "stuff/",
      "Tests\\Webclient\\Extension\\Log\\": "tests/"
    }
  }
}
